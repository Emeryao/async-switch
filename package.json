{
    "name": "async-switch",
    "displayName": "Async Switch",
    "description": "",
    "version": "0.0.2",
    "publisher": "Emeryao",
    "icon": "assets/icon.png",
    "engines": {
        "vscode": "^1.17.0"
    },
    "keywords": [
        "async",
        "switch",
        "typescript",
        "async-switch",
        "async switch"
    ],
    "categories": [
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "onCommand:async-switch.toggle"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "async-switch.toggle",
                "title": "Toggle Async",
                "icon": "assets/icon.png"
            }
        ],
        "keybindings": [
            {
                "command": "async-switch.toggle",
                "key": "ctrl+t ctrl+a",
                "mac": "cmd+t cmd+a",
                "when": "editorTextFocus && !editorReadonly"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Emeryao/async-switch.git"
    }
}
